#include<bits/stdc++.h>
using namespace std;
int main()
{
        int n,sum=0;
        cin>>n;
        int a[n];
        for(int i=1;i<=n;i++)
        {
                cin>>a[i];
        }
        for(int i=1;i<=n;i++)
        {
                sum = sum + a[i];
        }
        double f = (sum/(n*100.00))*100;cout<<f;
}
