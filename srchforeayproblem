#include<bits/stdc++.h>
using namespace std;
int main()
{
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++)
        {
                cin>>a[i];
        }
        int flag=0;
        for(int i = 0; i < n; i++)
        {
                if(a[i] == 1)
                {
                        flag =1;
                        break;
                }
        }
        if(flag == 1) cout<<"Hard\n";
        else cout<<"Easy\n";
        return 0;
}
